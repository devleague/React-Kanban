    "@progress/kendo-react-dropdowns": "^2.3.0",
    "@progress/kendo-react-intl": "^2.3.0",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.1",
    "bookshelf": "^0.13.3",
    "connect-flash": "^0.1.1",
    "connect-redis": "^3.4.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "flash": "^1.1.0",
    "gyp": "^0.5.0",
    "knex": "^0.15.2",
    "method-override": "^3.0.0",
    "mime": "^1.6.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.5.0",
    "react": "^16.5.2",
    "react-beautiful-dnd": "^9.0.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
